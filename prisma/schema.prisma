generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

enum Status{
  unedited
  edited
  done
}

model User {
  id          Int      @id @default(autoincrement())
  name        String
  title       String
  description String
  status      Status   @default(unedited)
  createdAt   DateTime @default(now())
  updateAt    DateTime @updatedAt
  
}